@model IEnumerable<Sea.Core.Vessel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <ul>
            @{
                if (Model != null)
                { 
                    foreach (var vessel in Model.ToList())
                    { 
                        <li>@vessel.Name</li>
                    }
                }
            }
        </ul>
    </div>
</body>
</html>
